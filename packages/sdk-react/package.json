{
  "name": "@citymapper/sdk-react",
  "version": "0.0.1-alpha.0",
  "private": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Citymapper",
    "email": "developer@citymapper.com"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/citymapper/sdk-web.git",
    "directory": "packages/sdk-react"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "barrel": "barrelsby --delete --directory ./src --exclude \"mock|test|jest|utils\" --exportDefault",
    "build": "yarn barrel && yarn build:clean && yarn build:esm && yarn build:cjs && yarn build:typings",
    "build:clean": "rm -rf dist",
    "build:esm": "tsc --project tsconfig.json --module es6 --outDir dist/esm",
    "build:cjs": "tsc --project tsconfig.json --module commonjs --outDir dist/cjs",
    "build:typings": "tsc --project tsconfig.json --declaration --outDir dist/typings --emitDeclarationOnly",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "clean": "rm -rf node_modules && rm -rf dist",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@emotion/is-prop-valid": "^1.1.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "chroma-js": "^2.4.2",
    "luxon": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@emotion/jest": "^11.9.3",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-webpack4": "^6.5.8",
    "@storybook/manager-webpack4": "^6.5.8",
    "@storybook/react": "^6.5.8",
    "@storybook/testing-library": "^0.0.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/chroma-js": "^2.1.4",
    "@types/jest": "^28.1.4",
    "@types/luxon": "^2.3.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-loader": "^8.2.5",
    "barrelsby": "^2.3.4",
    "eslint": "^8.18.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3"
  }
}
